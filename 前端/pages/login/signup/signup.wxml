<!--pages/login/signup/signup.wxml-->
<view class="container">
  <view class="container2">
    <image class="logo" src="data:image/svg+xml;base64,"></image>
    <text style="font-weight: bold; font-size: 40rpx;">点评熊</text>
  </view>
  <form bindsubmit="create_signup">
    <view style="background-color: #eeeeee; border-radius: 32rpx;">
      <view class="textbox">
        <mp-icon icon="me" color="black" size="{{25}}" class="icon"></mp-icon>
        <input class="text-input" type="text" name="username" placeholder="设置您的用户名" value="{{username}}"></input>
      </view>
      <view class="textbox">
        <mp-icon icon="lock" color="black" size="{{25}}" class="icon"></mp-icon>
        <input class="text-input" type="password" name="password" placeholder="设置您的密码" value="{{password}}"></input>
      </view>
    </view>
    <button bindtap="goto_index" class="btn-login" form-type="submit"><text>注册</text></button>
  </form>
</view>